{
  "tabnine.experimentalAutoImports": true,
  "redhat.telemetry.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "workbench.colorTheme": "Tokyo Night",
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "editor.formatOnSave": true,
  "auto-close-tag.SublimeText3Mode": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "testExplorer.useNativeTesting": true,
  "highlight-matching-tag.highlightFromContent": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "solargraph.autoformat": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "vscode-erb-beautify.bundlerPath": "/home/rinyaresu/.asdf/shims/bundler",
  "vscode-erb-beautify.executePath": "/home/rinyaresu/.asdf/shims/htmlbeautifier",
  "vscode-erb-beautify.keepBlankLines": 1,
  "customizeUI.activityBar": "bottom",
  "todo-tree.filtering.excludedWorkspaces": ["**/node_modules"],
  "indentRainbow.includedLanguages": ["*", "html", "erb"],
  "auto-close-tag.fullMode": true,
  "color-highlight.matchWords": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "vim.leader": "<space>",
  "editor.minimap.enabled": false,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "ruby",
    "lang-cfml",
    "cfml",
    "HTML (Eex)"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO:",
    "XXX",
    "[ ]",
    "[x]",
    "TODO"
  ],
  "todohighlight.include": [
    "**/*.rb",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss",
    "**/*.erb"
  ],
  "color-highlight.languages": [
    "scss",
    "css",
    "html",
    "javascript",
    "erb",
    "ruby",
    "yaml",
    "json",
    "*"
  ],
  "[erb]": {
    "editor.formatOnSave": true
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "ruby": true,
    "javascript": true
  },
  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "html": "html",
    "erb": "erb"
  },
  "tailwindCSS.emmetCompletions": true,
  "files.associations": {
    "*.html.erb": "erb"
  },
  "emmet.includeLanguages": {
    "*.html.erb": "html",
    "erb": "html"
  },
  "workbench.iconTheme": "material-icon-theme",
  "customizeUI.stylesheet": {
    ".tabs-container ": "padding-top:7px; padding-left:2px; padding-bottom:5px",
    ".tabs-container > .tab": "border-radius: 7px 7px 7px 7px; height: 20px; margin-left: 5px; padding-right:4px",
    ".monaco-editor .suggest-widget": "border-radius: 10px 10px 10px 10px; overflow:hidden;"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open"
}
